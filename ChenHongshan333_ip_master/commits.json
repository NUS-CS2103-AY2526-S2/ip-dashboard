leTypesAndContributionMap":{"gradle":{"insertions":6,"deletions":0},"txt":{"insertions":1,"deletions":2},"java":{"insertions":10,"deletions":4}}},{"hash":"86e80da9902d01e9ac7a3c2e6cd519d2747f8629","isMergeCommit":true,"messageTitle":"Merge branch \u0027branch-A-CheckStyle\u0027","messageBody":"","tags":["A-CheckStyle"],"fileTypesAndContributionMap":{}}]},{"date":"2026-02-06T00:00:00","commitResults":[{"hash":"7260ae350d31f75325b3120377be0dfd2190210a","isMergeCommit":false,"messageTitle":"Upgrade the CLI to GUI by following the tutorial, with basic interface implemented","messageBody":"","tags":["backup/Level-10-before-rewrite"],"fileTypesAndContributionMap":{"gradle":{"insertions":17,"deletions":1},"txt":{"insertions":2,"deletions":2},"fxml":{"insertions":59,"deletions":0},"java":{"insertions":303,"deletions":140}}},{"hash":"ee23aac2c3674bea5d50b581bb0bd43f8b4a2ef8","isMergeCommit":true,"messageTitle":"Merge branch \u0027branch-Level-10\u0027 (Level-10)","messageBody":"","tags":["Level-10","A-newJar"],"fileTypesAndContributionMap":{}}]},{"date":"2026-02-09T00:00:00","commitResults":[{"hash":"ca127f202a78af8be9bbb4ff4d9bdf039670e272","isMergeCommit":false,"messageTitle":"Add assertions to document Storage invariants","messageBody":"Storage relies on internal assumptions such as non-null dependencies\nand a single-line on-disk task format.\n\nWhen these assumptions are violated, failures can surface later as hard\nto trace bugs (e.g., null tasks, corrupted persisted format).\n\nLet\u0027s use Java\u0027s assert feature to document key invariants at critical\npoints in Storage.\n\nThis makes developer assumptions explicit and helps catch logic errors\nearly during development, without changing user-facing behavior when\nassertions are disabled.\n","tags":["A-FullCommitMessage"],"fileTypesAndContributionMap":{"java":{"insertions":32,"deletions":2}}},{"hash":"1e0a9d0e1ed07febf6edff85ff745168438d54e8","isMergeCommit":false,"messageTitle":"Refactor code to improve readability and maintainability","messageBody":"Some parts of the codebase contain duplicated logic, inconsistent\nnaming, and long methods that mix multiple responsibilities.\n\nThese issues make the code harder to read, reason about, and modify\nsafely, especially when adding new features or debugging edge cases.\n\nLet\u0027s refactor the implementation to align with the code quality\nprinciples covered so far (e.g., avoid duplication, keep methods small,\nuse meaningful names, and enforce clear class responsibilities).\n\nThe refactoring focuses on improving structure and clarity while\npreserving the existing external behavior, so future changes can be\nmade with lower risk.\n\nNo functional changes are intended in this commit.\n","fileTypesAndContributionMap":{"java":{"insertions":22,"deletions":20}}},{"hash":"7c3c95fd80e133ca49581376e316c7052576cc16","isMergeCommit":false,"messageTitle":"Use Java Streams for Storage load transformations","messageBody":"Storage reads tasks by iterating over file lines and manually building\ncollections.\n\nThe loop-based approach works but obscures the intent of the code and\nscatters filtering and transformation logic across multiple steps.\n\nLet\u0027s refactor Storage to use Java Streams where it fits naturally:\ntransform file lines to tasks using a stream pipeline, and transform\ntasks to encoded lines in save() via mapping and collecting.\n\nUsing Streams makes the data flow clearer and reduces boilerplate while\npreserving existing behavior such as skipping empty lines and warning on\ncorrupted entries.\n","fileTypesAndContributionMap":{"java":{"insertions":30,"deletions":23}}},{"hash":"51f7e1e3a50c44128ce5c079c1a9bc5f4ce83e7c","isMergeCommit":true,"messageTitle":"Merge branch-A-Assertions to master","messageBody":"Do a normal merge request fom branch-A-Assertions to master","tags":["A-Assertions"],"fileTypesAndContributionMap":{}},{"hash":"de0f4780f0d9a25ecfeb61e06455a7dad87f7ed7","isMergeCommit":true,"messageTitle":"Merge branch \u0027master\u0027 into branch-A-CodeQuality","messageBody":"","fileTypesAndContributionMap":{}},{"hash":"e179036a5839ad7ee739d464f54b8164910115f5","isMergeCommit":true,"messageTitle":"Solve conflicts in Storage class when merging master to branch-A-Streams","messageBody":"","fileTypesAndContributionMap":{}},{"hash":"2c87c8a5ddec15e39cbd2ff0be57d2e8ea5e1388","isMergeCommit":true,"messageTitle":"Merge pull request from branch-A-CodeQuality to master","messageBody":"Branch-A-CodeQuality","tags":["A-CodeQuality"],"fileTypesAndContributionMap":{}},{"hash":"d96a89c2aa0673bd43e96c4a5def69a19abc1e9b","isMergeCommit":true,"messageTitle":"Merge pull request branch-A-Streams to master","messageBody":"Branch-A-Streams","tags":["A-Streams"],"fileTypesAndContributionMap":{}},{"hash":"3d332557f648e793dfa5b07a987c97aeab030e57","isMergeCommit":false,"messageTitle":"Implement B-Snooze extension","messageBody":"","fileTypesAndContributionMap":{"txt":{"insertions":2,"deletions":2},"java":{"insertions":199,"deletions":12}}},{"hash":"515f64a14fc4b1758481647d6b865e28d5cac8c6","isMergeCommit":true,"messageTitle":"Merge branch \u0027branch-BCD-Extension\u0027","messageBody":"","tags":["BCD-Extension"],"fileTypesAndContributionMap":{}}]},{"date":"2026-02-15T00:00:00","commitResults":[{"hash":"50ebba866818fab269fd74c3ca17a4cde2b45229","isMergeCommit":false,"messageTitle":"Add and empty AI.md file","messageBody":"","fileTypesAndContributionMap":{"md":{"insertions":0,"deletions":0}}}]}]},"authorFileTypeContributionMap":{"ChenHongshan333":{"java":1578,"md":0,"fxml":59,"sh":0,"bat":1,"gradle":24,"txt":13}},"authorContributionVariance":{"ChenHongshan333":46979.82},"authorDisplayNameMap":{"ChenHongshan333":"CS2103T-T16-3 CHEN..SHAN"}}
