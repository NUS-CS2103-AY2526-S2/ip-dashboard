s":["A-CodingStandard"],"fileTypesAndContributionMap":{}},{"hash":"40aad7007b332b3dceefcff2c8aa99fb9a262171","isMergeCommit":true,"messageTitle":"Merge branch \u0027branch-Level-9\u0027","messageBody":"","tags":["Level-9"],"fileTypesAndContributionMap":{}}]},{"date":"2026-02-02T00:00:00","commitResults":[{"hash":"50af1ab03496ee5630c9f63417817adbf0a96a04","isMergeCommit":false,"messageTitle":"Backup CLI Before GUI","messageBody":"","fileTypesAndContributionMap":{"txt":{"insertions":2,"deletions":0},"java":{"insertions":3,"deletions":0}}},{"hash":"11ce2bf7fc9e1758de5c9ab0fa849c56f29b3e8a","isMergeCommit":false,"messageTitle":"Update data file","messageBody":"","fileTypesAndContributionMap":{"txt":{"insertions":3,"deletions":4},"fxml":{"insertions":66,"deletions":0},"java":{"insertions":16,"deletions":0}}},{"hash":"3b6e4ebf6f982fff5ab5b1783fa5ec7ef7a40f77","isMergeCommit":false,"messageTitle":"Level-10: GUI","messageBody":"","fileTypesAndContributionMap":{"gradle":{"insertions":17,"deletions":1},"java":{"insertions":209,"deletions":61}}},{"hash":"febfea2084d5869f9606e81951f360c2f8408fcd","isMergeCommit":true,"messageTitle":"Merge branch \u0027branch-Level-10\u0027","messageBody":"","tags":["Level-10"],"fileTypesAndContributionMap":{}}]},{"date":"2026-02-08T00:00:00","commitResults":[{"hash":"63daa8bfc54289490e937507c9e610a4fd8dc174","isMergeCommit":false,"messageTitle":"Add JavaFX GUI and integrate it with BoffBot logic","messageBody":"This commit introduces a JavaFX-based graphical user interface for BoffBot.\nThe GUI replaces the previous CLI-based interaction while preserving all\nexisting chatbot logic and command behavior.\n\nThe application now uses a MainWindow controller and DialogBox components\ndefined using FXML to handle user input and display bot responses visually.\nUser commands are forwarded to BoffBot through a single entry point, and\nresponses are rendered as dialog boxes instead of console output.\n\nJavaFX dependencies are configured using the Gradle JavaFX plugin, and the\napplication is packaged as a runnable fat JAR to ensure compatibility on\nsystems without JavaFX installed.\n","tags":["A-FullCommitMessage"],"fileTypesAndContributionMap":{"txt":{"insertions":1,"deletions":1}}},{"hash":"ff881eb1f764fe6a9be2d80a30247624eabd80a7","isMergeCommit":false,"messageTitle":"Add assertions to document internal invariants","messageBody":"Program logic relies on certain conditions being true at runtime\n(e.g., non-null descriptions, valid index ranges, consistent date order).\nWithout explicit documentation of these assumptions in code, it\nbecomes harder for future developers (and graders) to understand\ninternal expectations.\n\nThis commit adds Java `assert` statements in key places such as:\n\n• Task constructor to ensure descriptions are non-null and non-empty.\n• Deadline and Event constructors to validate non-null date strings\n  and date relationships.\n• TaskList methods to document valid index boundaries.\n• Parser.parse method to assert non-null input and dependencies.\n• Storage to verify non-null file paths and save lists.\n\nAssertions are not used for user input validation; existing\nexception-based handling remains intact. As assertions are disabled\nby default in Java, this does not change program behavior for normal\nusers.\n","fileTypesAndContributionMap":{"txt":{"insertions":2,"deletions":1},"java":{"insertions":30,"deletions":2}}},{"hash":"2f8e4471107fa8985efd909ee6a2fa1c50155af5","isMergeCommit":true,"messageTitle":"Merge pull request #1 from x3BoffiNg/branch-A-Assertions","messageBody":"Add assertions to document internal invariants","tags":["A-Assertions"],"fileTypesAndContributionMap":{}}]}]},"authorFileTypeContributionMap":{"x3BoffiNg":{"java":905,"md":0,"fxml":66,"sh":0,"bat":1,"gradle":20,"txt":12}},"authorContributionVariance":{"x3BoffiNg":33036.383},"authorDisplayNameMap":{"x3BoffiNg":"CS2103-F11-1 CLAR..HARN"}}
